{
	"name": "paperboypwa",
	"description": "PWA version for Paperboy app",
	"version": "1.0.0",
	"author": "Jo√£o Dias <contacto@joaodias.me>",
	"dependencies": {
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-proposal-optional-chaining": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@pwabuilder/pwainstall": "^1.6.4",
		"@types/lodash": "^4.14.158",
		"axios": "^0.19.2",
		"classnames": "^2.2.6",
		"cypress-dark": "^1.7.14",
		"date-fns": "^2.15.0",
		"formik": "^2.1.5",
		"gatsby": "^2.24.23",
		"gatsby-cli": "^2.12.78",
		"gatsby-image": "^2.4.14",
		"gatsby-plugin-google-analytics": "^2.3.13",
		"gatsby-plugin-manifest": "^2.4.21",
		"gatsby-plugin-netlify": "^2.3.11",
		"gatsby-plugin-offline": "^3.2.21",
		"gatsby-plugin-react-helmet": "^3.3.10",
		"gatsby-plugin-sass": "^2.3.12",
		"gatsby-plugin-styled-components": "^3.3.10",
		"gatsby-plugin-typescript": "^2.4.16",
		"history": "^5.0.0",
		"immer": "^7.0.7",
		"intersection-observer": "^0.11.0",
		"lodash": "^4.17.19",
		"node-sass": "^4.14.1",
		"polished": "^3.6.5",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-device-detect": "^1.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.1.0",
		"react-lazy-images": "^1.1.0",
		"react-redux": "^7.2.1",
		"react-share": "^4.2.1",
		"react-spring": "^8.0.27",
		"react-swipeable-views": "^0.13.9",
		"react-use": "^15.3.3",
		"redux": "^4.0.5",
		"redux-persist": "^6.0.0",
		"redux-thunk": "^2.3.0",
		"styled-components": "^5.1.1",
		"yup": "^0.29.2"
	},
	"keywords": [
		"gatsby",
		"redux"
	],
	"license": "MIT",
	"scripts": {
		"reinstall": "rm -rf node_modules && rm -rf ./.cache && npm run rebuild",
		"build": "gatsby build",
		"develop": "gatsby develop",
		"clean": "gatsby clean",
		"prettier:base": "prettier --parser typescript",
		"format:fix": "npm run prettier:base -- --write 'src/**/*.{ts,tsx, js, jsx, json}'",
		"format:lint": "eslint --fix src/**/*.tsx",
		"prebuild": "npm run format:fix && npm run format:lint && npm run test",
		"start": "gatsby develop",
		"test": "jest --env=jsdom",
		"test:watch": "jest --watchAll --env=jsdom",
		"test:cypress": "cypress run",
		"test:cypress-ui": "cypress open",
		"test:cypress-coverage": "npx nyc report --reporter=text-summary",
		"type-check": "tsc --noEmit"
	},
	"devDependencies": {
		"@babel/core": "^7.11.0",
		"@babel/polyfill": "^7.10.4",
		"@cypress/code-coverage": "^3.8.1",
		"@testing-library/cypress": "^6.0.0",
		"@testing-library/jest-dom": "^5.11.2",
		"@testing-library/react": "^10.4.7",
		"@types/classnames": "^2.2.10",
		"@types/jest": "^26.0.8",
		"@types/node": "^14.0.27",
		"@types/react": "^16.9.44",
		"@types/react-dom": "^16.9.8",
		"@types/react-helmet": "^6.0.0",
		"@types/react-redux": "^7.1.9",
		"@types/react-share": "^3.0.3",
		"@types/react-swipeable-views": "^0.13.0",
		"@types/react-window": "^1.8.2",
		"@types/styled-components": "^5.1.2",
		"@types/yup": "^0.29.3",
		"@typescript-eslint/eslint-plugin": "^3.7.1",
		"@typescript-eslint/parser": "^3.7.1",
		"babel-jest": "^26.2.2",
		"babel-plugin-styled-components": "^1.11.1",
		"babel-preset-gatsby": "^0.5.4",
		"create-react-component-folder": "^0.3.2",
		"cypress": "^4.12.1",
		"cypress-axe": "^0.8.1",
		"eslint": "^7.6.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-import-resolver-typescript": "^2.2.0",
		"eslint-plugin-chai-friendly": "^0.6.0",
		"eslint-plugin-cypress": "^2.11.1",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jest": "^23.20.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.20.5",
		"eslint-plugin-react-hooks": "^4.0.8",
		"gatsby-plugin-root-import": "^2.0.5",
		"husky": "^4.2.5",
		"identity-obj-proxy": "^3.0.0",
		"istanbul-lib-coverage": "^3.0.0",
		"jest": "^26.2.2",
		"js-logger": "^1.6.0",
		"lint-staged": "^10.2.11",
		"nyc": "^15.1.0",
		"prettier": "^2.0.5",
		"redux-devtools-extension": "^2.13.8",
		"ts-jest": "^26.1.4",
		"typescript": "^3.9.7",
		"typescript-styled-plugin": "^0.15.0",
		"webpack": "^4.44.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/joaotmdias/paperboy"
	},
	"browserslist": [
		"> 0.7%"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write",
			"git add"
		]
	},
	"jest": {
		"setupFilesAfterEnv": [
			"@testing-library/jest-dom",
			"@testing-library/jest-dom/extend-expect",
			"./setup-jest.js"
		],
		"globals": {
			"__PATH_PREFIX__": ""
		},
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(tsx?|jsx?)$": "ts-jest"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!(gatsby)/)",
			"static"
		],
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
		"testPathIgnorePatterns": [
			"node_modules",
			".cache",
			"static"
		],
		"moduleFileExtensions": [
			"js",
			"json",
			"jsx",
			"ts",
			"tsx"
		],
		"moduleNameMapper": {
			"^components/(.*)$": "<rootDir>/src/components/$1",
			"^containers/(.*)$": "<rootDir>/src/containers/$1",
			"^data/(.*)$": "<rootDir>/src/data/$1",
			"^helpers/(.*)$": "<rootDir>/src/helpers/$1",
			"typeface-*": "identity-obj-proxy",
			".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
			".+\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
		},
		"collectCoverage": true,
		"coverageReporters": [
			"lcov",
			"text",
			"html"
		]
	},
	"crcf": [
		"typescript",
		"scss",
		"createindex"
	]
}
