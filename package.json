{
	"name": "paperboypwa",
	"description": "PWA version for Paperboy app",
	"version": "1.0.0",
	"author": "Jo√£o Dias <contacto@joaodias.me>",
	"dependencies": {
		"@babel/preset-typescript": "^7.3.3",
		"@material-ui/core": "^4.1.1",
		"@types/lodash": "^4.14.134",
		"axios": "^0.19.0",
		"date-fns": "^1.30.1",
		"formik": "^1.5.7",
		"gatsby": "^2.10.0",
		"gatsby-cli": "^2.7.0",
		"gatsby-image": "^2.2.1",
		"gatsby-plugin-google-analytics": "^2.1.0",
		"gatsby-plugin-manifest": "^2.2.0",
		"gatsby-plugin-netlify": "^2.1.0",
		"gatsby-plugin-offline": "^2.2.0",
		"gatsby-plugin-react-helmet": "^3.1.0",
		"gatsby-plugin-sass": "^2.1.0",
		"gatsby-plugin-styled-components": "^3.1.0",
		"gatsby-plugin-typescript": "^2.1.0",
		"history": "^4.9.0",
		"immer": "^3.1.3",
		"intersection-observer": "^0.7.0",
		"node-sass": "^4.12.0",
		"polished": "^3.4.1",
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-device-detect": "^1.6.2",
		"react-dom": "^16.8.6",
		"react-helmet": "^5.2.1",
		"react-lazy-images": "^1.1.0",
		"react-redux": "^7.1.0",
		"react-spring": "^8.0.25",
		"react-swipeable-views": "^0.13.3",
		"react-window": "^1.8.3",
		"redux": "^4.0.1",
		"redux-persist": "^5.10.0",
		"redux-thunk": "^2.3.0",
		"styled-components": "^4.3.2",
		"yup": "^0.27.0",
		"react-share": "^3.0.0"
	},
	"keywords": [
		"gatsby",
		"redux"
	],
	"license": "MIT",
	"scripts": {
		"build": "gatsby build",
		"start": "gatsby develop",
		"develop": "gatsby develop",
		"format:fix": "prettier --write '{__mocks__,src,test}/**/*.{ts,tsx}' '*.js'",
		"format:lint": "eslint --fix src/**/*.tsx",
		"test": "jest --watchAll --env=jsdom",
		"type-check": "tsc --noEmit",
		"prebuild": "yarn format:fix && yarn format:lint && yarn test"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/polyfill": "^7.4.4",
		"@testing-library/react": "^8.0.1",
		"@types/jest": "^24.0.15",
		"@types/node": "^12.0.10",
		"@types/react": "^16.8.22",
		"@types/react-dom": "^16.8.4",
		"@types/react-helmet": "^5.0.8",
		"@types/react-redux": "^7.1.0",
		"@types/react-share": "^2.1.1",
		"@types/react-swipeable-views": "^0.13.0",
		"@types/react-window": "^1.8.0",
		"@types/styled-components": "^4.1.16",
		"@types/yup": "^0.26.17",
		"@typescript-eslint/eslint-plugin": "^1.10.2",
		"@typescript-eslint/parser": "^1.10.2",
		"babel-jest": "^24.8.0",
		"babel-plugin-styled-components": "^1.10.1",
		"babel-preset-gatsby": "^0.2.0",
		"create-react-component-folder": "^0.1.30",
		"eslint": "^5.16.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-config-prettier": "^5.0.0",
		"eslint-import-resolver-typescript": "^1.1.1",
		"eslint-plugin-import": "^2.17.3",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-react": "^7.13.0",
		"eslint-plugin-react-hooks": "^1.6.0",
		"husky": "^2.4.1",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.8.0",
		"jest-dom": "^3.5.0",
		"lint-staged": "^8.2.1",
		"prettier": "^1.18.2",
		"redux-devtools-extension": "^2.13.8",
		"ts-jest": "^24.0.2",
		"typescript": "^3.5.2",
		"typescript-styled-plugin": "^0.14.0",
		"webpack": "^4.35.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/joaotmdias/paperboy"
	},
	"browserslist": [
		"> 0.7%"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write",
			"git add"
		]
	},
	"jest": {
		"setupFilesAfterEnv": [
			"./setup-jest.js"
		],
		"globals": {
			"__PATH_PREFIX__": ""
		},
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(tsx?|jsx?)$": "ts-jest"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!(gatsby)/)"
		],
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
		"testPathIgnorePatterns": [
			"node_modules",
			".cache"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js"
		],
		"moduleNameMapper": {
			"typeface-*": "identity-obj-proxy",
			".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
			".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
		},
		"collectCoverage": false,
		"coverageReporters": [
			"lcov",
			"text",
			"html"
		]
	},
	"crcf": [
		"typescript",
		"scss",
		"createindex"
	]
}
