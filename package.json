{
	"name": "paperboypwa",
	"description": "PWA version for Paperboy app",
	"version": "1.0.0",
	"author": "Jo√£o Dias <contacto@joaodias.me>",
	"dependencies": {
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-proposal-optional-chaining": "^7.7.5",
		"@babel/preset-env": "^7.7.7",
		"@babel/preset-react": "^7.7.4",
		"@babel/preset-typescript": "^7.7.7",
		"@material-ui/core": "^4.8.3",
		"@types/lodash": "^4.14.149",
		"axios": "^0.19.0",
		"date-fns": "^2.8.1",
		"formik": "^2.1.1",
		"gatsby": "^2.18.18",
		"gatsby-cli": "^2.8.23",
		"gatsby-image": "^2.2.37",
		"gatsby-plugin-google-analytics": "^2.1.31",
		"gatsby-plugin-manifest": "^2.2.34",
		"gatsby-plugin-netlify": "^2.1.30",
		"gatsby-plugin-offline": "^3.0.30",
		"gatsby-plugin-react-helmet": "^3.1.18",
		"gatsby-plugin-sass": "^2.1.26",
		"gatsby-plugin-styled-components": "^3.1.16",
		"gatsby-plugin-typescript": "^2.1.23",
		"history": "^4.10.1",
		"immer": "^5.1.0",
		"intersection-observer": "^0.7.0",
		"node-sass": "^4.13.0",
		"polished": "^3.4.2",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-device-detect": "^1.11.14",
		"react-dom": "^16.12.0",
		"react-helmet": "^5.2.1",
		"react-lazy-images": "^1.1.0",
		"react-redux": "^7.1.3",
		"react-share": "^4.0.0",
		"react-spring": "^8.0.27",
		"react-swipeable-views": "^0.13.3",
		"react-window": "^1.8.5",
		"redux": "^4.0.5",
		"redux-persist": "^6.0.0",
		"redux-thunk": "^2.3.0",
		"styled-components": "^4.4.1",
		"yup": "^0.28.0"
	},
	"keywords": [
		"gatsby",
		"redux"
	],
	"license": "MIT",
	"scripts": {
		"build": "gatsby build",
		"start": "gatsby develop",
		"develop": "gatsby develop",
		"format:fix": "prettier --write '{__mocks__,src,test}/**/*.{ts,tsx}' '*.js'",
		"format:lint": "eslint --fix src/**/*.tsx",
		"test": "jest --env=jsdom",
		"test:watch": "jest --watchAll --env=jsdom",
		"type-check": "tsc --noEmit",
		"prebuild": "npm run format:fix && npm run format:lint && npm run test"
	},
	"devDependencies": {
		"@babel/core": "^7.7.7",
		"@babel/polyfill": "^7.7.0",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.4.0",
		"@types/jest": "^24.0.25",
		"@types/node": "^13.1.4",
		"@types/react": "^16.9.17",
		"@types/react-dom": "^16.9.4",
		"@types/react-helmet": "^5.0.15",
		"@types/react-redux": "^7.1.5",
		"@types/react-share": "^3.0.3",
		"@types/react-swipeable-views": "^0.13.0",
		"@types/react-window": "^1.8.1",
		"@types/styled-components": "^4.4.1",
		"@types/yup": "^0.26.27",
		"@typescript-eslint/eslint-plugin": "^2.14.0",
		"@typescript-eslint/parser": "^2.14.0",
		"babel-jest": "^24.9.0",
		"babel-plugin-styled-components": "^1.10.6",
		"babel-preset-gatsby": "^0.2.26",
		"create-react-component-folder": "^0.1.30",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.9.0",
		"eslint-import-resolver-typescript": "^2.0.0",
		"eslint-plugin-import": "^2.19.1",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.17.0",
		"eslint-plugin-react-hooks": "^2.3.0",
		"husky": "^3.1.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.9.0",
		"lint-staged": "^9.5.0",
		"prettier": "^1.19.1",
		"redux-devtools-extension": "^2.13.8",
		"ts-jest": "^24.2.0",
		"typescript": "^3.7.4",
		"typescript-styled-plugin": "^0.15.0",
		"webpack": "^4.41.5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/joaotmdias/paperboy"
	},
	"browserslist": [
		"> 0.7%"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write",
			"git add"
		]
	},
	"jest": {
		"setupFilesAfterEnv": [
			"@testing-library/jest-dom",
			"@testing-library/jest-dom/extend-expect",
			"./setup-jest.js"
		],
		"globals": {
			"__PATH_PREFIX__": ""
		},
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(tsx?|jsx?)$": "ts-jest"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!(gatsby)/)",
			"static"
		],
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
		"testPathIgnorePatterns": [
			"node_modules",
			".cache",
			"static"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx"
		],
		"moduleNameMapper": {
			"typeface-*": "identity-obj-proxy",
			".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
			".+\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
		},
		"collectCoverage": true,
		"coverageReporters": [
			"lcov",
			"text",
			"html"
		]
	},
	"crcf": [
		"typescript",
		"scss",
		"createindex"
	]
}
